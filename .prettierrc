{
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "importOrder": [
        "<THIRD_PARTY-MODULES>",
        "^@/components/(.*)$",
        "^@/screens/(.*)$",
        "^@/ui/(.*)$",

        "^@/hooks/(.*)$",
        "^@/shared/(.*)$",
        "^@/services/(.*)$",
        "^@/utils/(.*)$",
        "^@/config/(.*)$",
        "^@/store/(.*)$",
        "^@/assets/(.*)$",
        "^./(.*)",
        "^../(.*)",
        "(.scss)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}